{
  "sources": [
    {
      "url": "https://raw.githubusercontent.com/olbat/ut1-blacklists/master/blacklists/social_networks/domains",
      "format": "domains"
    }
  ],
  "sanitizers": [
    "iana-tlds",
    "url-shorteners",
    "bin-hostnames",
    "bin-domains",
    "ddns"
  ],
  "exclusions": [
    "*.telegram.org",
    "*.whatsapp.com",
    "*.xx.fbcdn.net",
    "*.edge-mqtt.facebook.com",
    "*.graph.facebook.com"
  ],
  "inclusions": [
    "m.facebook.com",
    "www.facebook.com"
  ]
}
